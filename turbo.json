{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        "build/**"
      ],
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "lint:fix": {
      "outputs": [],
      "cache": false
    },
    "format": {
      "outputs": [],
      "cache": false
    },
    "deploy-stack": {
      "outputs": [],
      "cache": false
    },
    "build-image": {
      "outputs": [],
      "cache": false
    }
  }
}